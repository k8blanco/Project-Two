<h3>Recommendations:</h3>
<h2>testing</h2>
<div class="container">
    <div class="card">
        <ul id="results-list" class="list-group">
            {{#each booksArray}}
            <li data-id="{{this.gbId}}" class="list-group-item">
                <span>{{this.gbInfo.title}}</span>
                <button class="btn">Heart!</button>
            </li>
            {{/each}}
        </ul>
    </div>
</div>



<div class="carousel">
    {{#each booksArray}}
    <a class="carousel-item"><img src="{{this.gbInfo.imageLinks.thumbnail}}"></a>
    {{/each}}
